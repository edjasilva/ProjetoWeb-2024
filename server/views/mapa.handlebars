<header>


  <div class="container">
    <div class="row justify-content-around">
      <div class="col-4" style="display: none;">
          <ul>
              {{#each data}}
                  <li><a href="#">{{name}}</a>
                      <span class="badge rounded-pill text-bg-dark">{{rating}} ({{num_reviews}})</span>
                      <i class="bi-pin-map-fill map-pin" lon="{{location.coordinates.[0]}}"
                          lat="{{location.coordinates.[1]}}"  id="{{id}}" name="{{name}}"  rating="{{rating}}" 
                          street="{{street}}" zipcode="{{zipcode}}" spot_type="{{spot_type}}" pic_url="{{pic}}"></i>
                  </li>
              {{/each}}
          </ul>
      </div>


      
      <div class="col my-3 ">
        <div id="map" class="custom-map"></div>
           <a href="https://lisbonspots.onrender.com/">
          <button id="crossButton">&#10006;</button>
        </a>
      </div>
    </div>
    
  </div>
</header>
